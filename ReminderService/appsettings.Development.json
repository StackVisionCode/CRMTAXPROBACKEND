{
  "AllowedHosts": "*",

  "Notify": {
    "BaseUrl": "http://localhost:5000",
    "SendPath": "/email/api/notify"
  },

  "Databases": {
    "ReminderDb": "ReminderDb",
    "QuartzDb": "QuartzDb"
  },

  "Quartz": {
    "quartz.serializer.type": "json"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
