{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HybridCache": {
    "Redis": {
      "ConnectionString": "localhost:6379",
      "KeyPrefix": "taxapp",
      "Database": 0,
      "DefaultExpiry": "00:30:00",
      "CommandTimeout": "00:00:05",
      "ConnectTimeout": "00:00:05"
    },
    "Local": {
      "SizeLimit": 4096,
      "ExpirationScanFrequency": "00:02:00",
      "DefaultExpiry": "00:15:00",
      "CompactionPercentage": 0.25
    },
    "HealthCheck": {
      "CheckInterval": "00:00:30",
      "FailureThreshold": 3,
      "RecoveryInterval": "00:01:00",
      "HealthCheckTimeout": "00:00:02"
    }
  },
  "AllowedHosts": "*"
}
